<configuration debug="true">
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>
                %d{hh:mm:ss.SSS} [%thread] %-10level %logger{36} - %msg%n
            </pattern>
        </encoder>
    </appender>
    <root level="trace">
        <appender-ref ref="console"/>
    </root>
</configuration>